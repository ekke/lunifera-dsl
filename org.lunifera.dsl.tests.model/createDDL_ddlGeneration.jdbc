CREATE TABLE ADDON (UUID VARCHAR(255) NOT NULL, DESCRIPTION VARCHAR(255), CAR VARCHAR(255), PRIMARY KEY (UUID))
CREATE TABLE CAR (UUID VARCHAR(255) NOT NULL, FINISHING_DATE DATE, NUMBER VARCHAR(255), OWNER VARCHAR(255), PRIMARY KEY (UUID))
CREATE TABLE PERSON (UUID VARCHAR(255) NOT NULL, FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), DISPOSED SMALLINT DEFAULT 0, HOMEADDRESS_POSTALCODE VARCHAR(255), HOMEADDRESS_STREETNAME VARCHAR(255), WORKADDRESS_POSTALCODE VARCHAR(255), WORKADDRESS_STREETNAME VARCHAR(255), PRIMARY KEY (UUID))
ALTER TABLE ADDON ADD CONSTRAINT FK_ADDON_CAR FOREIGN KEY (CAR) REFERENCES CAR (UUID)
ALTER TABLE CAR ADD CONSTRAINT FK_CAR_OWNER FOREIGN KEY (OWNER) REFERENCES PERSON (UUID)
