package org.lunifera.entitydsl.documentation.demo {
	
	/* Imports the original entity package */
	import org.lunifera.entitydsl.documentation.demo.*;
	

	datatype String jvmType java.lang.String;
	datatype long jvmType java.lang.Long;

	dto FooDto wraps Foo {
		inherit var id;
		inherit var name;
	}
	
	dto BarDto extends FooDto wraps Bar {
		inherit var number;
	}
	
}
