package org.example {
	
	datatype long jvmType Long as primitive;
	datatype String jvmType String;
	
	mapped superclass Base {
		id long id;
	}
	
	entity Foo extends Base {
		inheritance per subclass{}
		var String name;
		var ValueBean mybean;
	}
	 
	entity Bar extends Foo {
		var String barDescription;
		var Status status;
	}
	 
	bean ValueBean {
		var String description;
		var long value;
	}
	
	enum Status {
		ON, OFF
	}
	
} 
















